{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++-9 build active file",
			"command": "/usr/bin/g++-9",
			"args": [
				"-g",
				"${workspaceFolder}/*.cc",
				"${workspaceFolder}/*.cpp",
				"-o",
				"${workspaceFolder}/test"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "编译器: /usr/bin/g++-9"
		}
	]
}